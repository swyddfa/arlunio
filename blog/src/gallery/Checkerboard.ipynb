   "source": [
    "import arlunio.image as image\n",
    "import arlunio.mask as mask\n",
    "import arlunio.pattern as pattern\n",
    "\n",
    "grid = mask.Repeat(defn=pattern.Checker())\n",
    "img = image.fill(grid(width=1080, height=1080), background=\"white\")"
   ]
  }
